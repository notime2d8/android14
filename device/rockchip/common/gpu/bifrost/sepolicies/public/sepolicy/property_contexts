#
# Usage properties for the Arm reference Gralloc
#
# The Mali™ DDK reads these properties to obtain information about the values of the vendor
# usage bits. These vendor usage bits have the type BufferUsage defined in the AOSP file
# hardware/interfaces/graphics/common/1.0/types.hal. The values can be passed to
# the allocate function from the Android IAllocator interface.
#
# No AFBC vendor usage bit
ro.vendor.arm.gralloc.no_afbc_usage_flags u:object_r:vendor_arm_gralloc_prop:s0 exact uint
ro.vendor.arm.gralloc.no_afbc_usage_mask u:object_r:vendor_arm_gralloc_prop:s0 exact uint
# Force Back Buffer vendor usage bit
ro.vendor.arm.gralloc.force_back_buffer_usage_flags u:object_r:vendor_arm_gralloc_prop:s0 exact uint
ro.vendor.arm.gralloc.force_back_buffer_usage_mask u:object_r:vendor_arm_gralloc_prop:s0 exact uint
# Shared Access vendor usage bit
ro.vendor.arm.gralloc.shared_access_usage_flags u:object_r:vendor_arm_gralloc_prop:s0 exact uint
ro.vendor.arm.gralloc.shared_access_usage_mask u:object_r:vendor_arm_gralloc_prop:s0 exact uint
# RGBA AFRC vendor usage bits
ro.vendor.arm.gralloc.afrc_rgba_usage_flags u:object_r:vendor_arm_gralloc_prop:s0 exact string
ro.vendor.arm.gralloc.afrc_rgba_usage_mask u:object_r:vendor_arm_gralloc_prop:s0 exact uint
# YUV AFRC vendor usage bits
ro.vendor.arm.gralloc.afrc_luma_usage_flags u:object_r:vendor_arm_gralloc_prop:s0 exact string
ro.vendor.arm.gralloc.afrc_luma_usage_mask u:object_r:vendor_arm_gralloc_prop:s0 exact uint
ro.vendor.arm.gralloc.afrc_chroma_usage_flags u:object_r:vendor_arm_gralloc_prop:s0 exact string
ro.vendor.arm.gralloc.afrc_chroma_usage_mask u:object_r:vendor_arm_gralloc_prop:s0 exact uint

