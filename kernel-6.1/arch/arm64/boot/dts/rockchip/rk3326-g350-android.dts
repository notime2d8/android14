	vccio2-supply = <&vccio_sd>;
	/* CAM I/O Voltage */
	vccio3-supply = <&vcc_3v3>;
	/* LVDS/LCDC I/O Voltage */
	vccio4-supply = <&vcc_3v3>;
	/* I2S I/O Voltage */
	vccio5-supply = <&vcc_3v3>;
	/* eMMC I/O Voltage */
	vccio6-supply = <&vcc_3v3>;
};

&isp_mmu {
	status = "okay";
};

&mipi_dphy_rx0 {
	status = "disabled";
};

&mpp_srv {
	status = "okay";
};

&rkisp1 {
	status = "okay";
};

&spi0 {
	status = "disabled";
};

&pmu_io_domains {
