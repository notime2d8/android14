                reg = <0>;
                power-supply = <&vcc3v3_lcd0_n>;
		backlight = <&backlight>;
		reset-gpios = <&gpio4 RK_PA0 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_rst_gpio>;
                
		reset-delay-ms = <160>;
		enable-delay-ms = <200>;
		prepare-delay-ms = <0>;
		init-delay-ms = <200>;
		unprepare-delay-ms = <20>;
		disable-delay-ms = <20>;

		width-mm = <68>;
		height-mm = <121>;
		
